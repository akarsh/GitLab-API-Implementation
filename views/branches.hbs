<ol class="breadcrumb">
  <li class="text-primary breadcrumb-item">Project name - {{projectBranchProjectName}}</li>
</ol>
<br />
<h3>Branches</h3>
<!-- Shows branches on the browser -->
<br/>
{{#each branches}}
<ul class="branches_list">
  <li>
    <form action="/projects/branches/builds" method="post" enctype="multipart/form-data">
      <fieldset>
        <input type="hidden" name="projectBranchId" value="{{../projectBranchId}}" />
        <input type="hidden" name="projectBranchProjectName" value="{{../projectBranchProjectName}}" />
        <input type="hidden" name="projectBranchProjectUserName" value="{{../projectBranchProjectUserName}}" />
        <input type="hidden" name="projectBranchName" value="{{this}}" />
        <input type="submit" value="{{this}}" />
      </fieldset>
    </form>
  </li>
</ul>
{{/each}}